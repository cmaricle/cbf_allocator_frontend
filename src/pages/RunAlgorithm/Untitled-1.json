function transformObject(inputObject) {
    const transformedObject = {};

    // Iterate over the keys of granted_test or requested_test (they should have the same keys)
    Object.keys(inputObject.quota_response.granted_quota).forEach(key => {
        // Create a new object with the structure { granted_test: ..., requested_test: ... }
        transformedObject[key] = {
            granted_test: inputObject.quota_response.granted_quota[key],
            requested_test: inputObject.quota_response.requested_quota[key]
        };
    });

    return transformedObject;
}